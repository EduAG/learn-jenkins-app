{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnJenkinsApp",
            "image": "590183774408.dkr.ecr.us-east-2.amazonaws.com/learnjenkinsapp:1.0.107",
            "portMappings": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::590183774408:role/ecsTaskExecutionRole"

}